OUTPUT_FORMAT("elf32-bigmips")
OUTPUT_ARCH("mips")
ENTRY(_start)
PHDRS
{
 text PT_LOAD AT (0x80002000);
 data PT_LOAD AT (0x8000a000);
}

SECTIONS
{
	__kvaddr_sp = 0x80008000;

	. = 0x80002000;

	.text :
	{
		*(.text*)
	}:text
	. = 0x8000a000;
	.data :
	{
		*(.data*)
		*(.rdata*)

	}:data

	.rodata :
	{
		*(.rodata*)
	}:data

	_gp = ALIGN(16);

	.bss :
	{
		*(.bss*)
	}:data
}
