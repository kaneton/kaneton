PROJECT         =               myproject
SRCS            =               main.c  \
                                chiche.c
OBJS            =               $(SRCS:.c=.o)
RM		=		rm -f

all:            $(OBJS)
        $(CC) $(OBJS) -o $(PROJECT)

main.o:         header.h

chiche.o:       header.h

clean:
        $(RM) $(OBJS) $(PROJECT)
