OUTPUT_FORMAT("elf64-littlemips")
ENTRY(bootloader_entry)

SECTIONS
{
	__start = 0xffffffffbfc00000;	
	. = __start;
	
	.text __start :
	{
		entry.o(.text)
		exception_handlers.o(.text)
	}

	.text :
	{
		*(.text)
	}

	__gp = ALIGN(8);
	__offset = __gp - __start;

	.data :
	{
		*(.data)
		*(.rdata)
		*(.rodata)
	}

	.bss :
	{
		*(.bss)
	}

	__bootloader_end = .;
}
